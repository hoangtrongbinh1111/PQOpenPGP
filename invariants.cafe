  -- **************************************************************************************************************************************************************************************************
  -- -- =====> AUTHEN OF MESSAGE : 

  op authenMess : Sys Prin Prin Prin RawMsg SessionKey MLK-SecretK ECDHK-SecretK -> Bool .
  eq authenMess(S,A2,A,B,M,K,Mlk-SK,Ecdhk-SK) = 
    (
      not(A = intru or B = intru) and
      msg(A2,A,B, (C1(B,Mlk-SK,Ecdhk-SK,K) || C2(A,M,K))) \in nw(S) and
      MLDSA-Verify(MLDSA-PubK(A), MLDSA-Sign(mldsa-getPrivate(MLDSA-PubK(A)), h(M)), h(M)) and
      EdDSA-Verify(EdDSA-PubK(A), EdDSA-Sign(eddsa-getPrivate(EdDSA-PubK(A)), h(M)), h(M)) and
      sdec(kcombine(
        mlk-decaps(mlk-encapsC(MLK-PubK(B),Mlk-SK), MLK-PriK(B)) || 
        ecdhk-decaps(ecdhk-encapsC(ECDHK-PubK(B),Ecdhk-SK), ECDHK-PriK(B)) || 
        ecdhk-encapsC(ECDHK-PubK(B),Ecdhk-SK) || ECDHK-PubK(B) || 
        mlk-encapsC(MLK-PubK(B),Mlk-SK) || MLK-PubK(B)), senc(KEK(B,Mlk-SK,Ecdhk-SK),K)) = K and
      not (mlk-decaps(mlk-encapsC(MLK-PubK(B),Mlk-SK), MLK-PriK(B)) \in leakscr(S)) and 
      not (ecdhk-decaps(ecdhk-encapsC(ECDHK-PubK(B),Ecdhk-SK), ECDHK-PriK(B)) \in leakscr(S)) and
      not (ECDHK-PriK(B) \in leakscr(S)) and 
      not (MLK-PriK(B) \in leakscr(S))
    ) implies 
      (msg(A,A,B, (C1(B,Mlk-SK,Ecdhk-SK,K) || C2(A,M,K))) \in nw(S)) .

