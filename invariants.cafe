  --   implies 
  --     (msg(A,A,B, (Ecdhk-Ci || Mlk-Ci || KC || senc(K,SIGN1 || SIGN2 || M))) \in nw(S)) .
  op inv3 : Sys Prin Prin RawMsg SessionKey MLK-SecretK ECDHK-SecretK -> Bool
  eq inv3(S,A,B,M,K,Mlk-SK,Ecdhk-SK) = 
    (not(A = intru or B = intru) and 
      not (ECDHK-PriK(B) \in leakscr(S)) and 
      not (MLK-PriK(B) \in leakscr(S)) and
      C2(A,M,K) \in knl(S)
      )
    implies 
      (msg(A,A,B, C1(B,Mlk-SK,Ecdhk-SK,K) || C2(A,M,K)) \in nw(S)) .
  
  op inv4 : Sys RawMsg SessionKey Data Data  -> Bool
  eq inv4(S,M,K,SIGN1,SIGN2) = 
    (M \in knl(S) and K \in knl(S) and SIGN1 \in knl(S) and SIGN2 \in knl(S))
    implies 
     senc(K,SIGN1 || SIGN2 || M) \in knl(S) .

